#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char nome[N][100];
char aluno[100];

void ordena(int N, int K){
  int n, m;
  for(n = 0; n <= N; n++){
    for(m = 0; m <= N; m++){
      if(strcmp(nome[n], nome[m]) > 0){
        char aux[100];
        strcpy(aux, nome[n]);
        strcpy(nome[n], nome[m];
        strcpy(nome[m], aux);
        }
    }
  }
strcpy(aluno, nome[K]);
printf("%s", aluno);
}

int main(){
  int N, K;
  scanf("%d %d", &N, &K);
  
  //leitura dos nomes dos alunos
  for(int l = 0; l < N; l++){
    fgets(nome[l], sizeof(nome[l]), stdin);
    nome[l][strcspn(nome[l), "\n")] = '\0';
  }
  
  ordenada(N, K);

  return 0;
}
