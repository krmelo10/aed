#include <stdio.h>
#include <time.h>
#define MAX 100

//função para ordenar o vetor
void insertion_sort(int v[], int tam){
    for (int i = 1; i < tam; i++){
        //guarda, temporariamente, o elemento que está em análise para ser ordenado
        int temp = v[i];
        //guarda o indíce anterior ao índice do elemento em análise para ordenação
        int ant = i - 1;

        /*percorre o vetor até o elemento anterior ao elemento em análise.
        move todos os elementos maiores que o elemento em análise uma posição à frente da posição atual        
        */
        while (ant >= 0 && v[ant] > temp) {
            v[ant + 1] = v[ant];
            ant = ant - 1;
        }
        v[ant + 1] = temp;
    }
}

//imprime o vetor ordenado
void imprime(int v[], int tam){
    for (int i = 0; i < tam; i++)
        printf("%d ", v[i]);
    printf("\n");
}

//função para verificar tempo de execução
void execucao(int v[], int tam){
    clock_t t;
    t = clock(); 
    insertion_sort(v, tam);
    t = clock() - t;
    printf("tempo de execução: %lf ms\n", ((double)t)/((CLOCKS_PER_SEC/1000)));
}

//função principal
int main(){
    int tam;
    scanf("%d", &tam);
    int v[MAX];
    for(int i = 0; i < tam; i++){
        printf("v[%d]: ", i + 1);
        scanf("%d", &v[i]);
    }
    insertion_sort(v, tam);
    imprime(v, tam);
    execucao(v, tam);

    return 0;
}
